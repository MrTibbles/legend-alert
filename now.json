{
  "public": false,
  "github": {
    "enabled": false
  },
  "builds": [
    { "src": "package.json", "use": "@now/static-build" },
    { "src": "package.json", "use": "@now/node" }
  ],
  "routes": [
    {
      "methods": ["GET"],
      "src": "/stats",
      "dest": "/public/index.html"
    },
    {
      "methods": ["GET"],
      "src": "/(.*)",
      "dest": "/public/$1"
    },
    {
      "methods": ["POST"],
      "src": "/graphql",
      "dest": "/dist/index.js"
    }
  ],
  "env": {
    "TRN_TOKEN": "b8b9affa-afeb-41dd-ade1-025f9d1f1f77"
  }
}